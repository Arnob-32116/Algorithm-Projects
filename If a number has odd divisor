#include <bits/stdc++.h>
using namespace std;

bool odd_divisor(long long int n){
    if ((n& (n - 1)) != 0)
        return true;
        else
        return false;
}
int main()
{       long long int n;
        cin >> n;
        if (odd_divisor(n))
        cout<<"YES"<<endl;
        else
        cout<<"NO"<<endl;

    
    return 0;
}
